    "@types/getenv": "^1.0.0",
    "@types/morgan": "^1.9.1",
    "@types/parse": "^2.12.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "getenv": "^1.0.0",
    "morgan": "^1.10.0",
    "parse": "^2.17.0",
    "parse-dashboard": "^2.1.0",
    "parse-server": "^4.10.11",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.3"
  }
}
